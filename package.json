{
  "name": "jivecake",
  "version": "0.0.0",
  "description": "jivecake",
  "scripts": {
    "build": "webpack --mode=production && webpack --config webpack-server.config.js --mode=production",
    "watch-web": "webpack --watch  --mode=development --config webpack-web.config.js",
    "watch-server": "webpack --watch --config webpack-server.config.js --mode=development",
    "test": "jest-webpack --clearCache && jest-webpack",
    "translation": "npm run translation-fr && npm run translation-ko",
    "translation-fr": "i18next-conv -l fr -s common/translation/fr.po -t common/translation/fr.json",
    "translation-ko": "i18next-conv -l ko -s common/translation/ko.po -t common/translation/ko.json",
    "translation-compile": "i18next-scanner --config i18next-scanner.config.js 'web/**/*.{js,jsx}'"
  },
  "author": "luis@trois.io",
  "license": "MIT",
  "dependencies": {

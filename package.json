{
  "name": "jivecake",
  "version": "0.0.0",
  "description": "jivecake",
  "scripts": {
    "build": "webpack --mode=production --config webpack-web.config.js && webpack --config webpack-server.config.js --mode=production",
    "watch-web": "webpack --watch --mode=development --config webpack-web.config.js",
    "watch-server": "webpack --watch --config webpack-server.config.js --mode=development",
    "translation": "npm run translation-fr && npm run translation-ko",
    "translation-fr": "i18next-conv -l fr -s common/translation/fr.po -t common/translation/fr.json",
    "translation-ko": "i18next-conv -l ko -s common/translation/ko.po -t common/translation/ko.json",
    "translation-compile": "i18next-scanner --config i18next-scanner.config.js 'web/**/*.{js,jsx}'"
  },
  "author": "luis@trois.io",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/plugin-transform-spread": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-brands-svg-icons": "^5.7.2",
    "@fortawesome/free-regular-svg-icons": "^5.7.2",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@sentry/browser": "^5.1.2",
    "@sentry/node": "^5.1.2",
    "@ungap/url-search-params": "^0.1.2",
    "ajv": "^6.9.1",
    "autoprefixer": "^9.4.7",
    "aws-sdk": "^2.449.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-react-css-modules": "^5.2.4",
    "babel-polyfill": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "compressorjs": "^1.0.5",
    "css-loader": "^2.1.0",
    "eslint": "^5.14.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
